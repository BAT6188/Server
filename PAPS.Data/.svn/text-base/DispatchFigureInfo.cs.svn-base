using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace PAPS.Data
{
    /// <summary>
    /// 指纹分发
    /// </summary>
    public class DispatchFigureInfo
    {
        /// <summary>
        /// 设备guid
        /// </summary>
        public Guid DeviceId { get; set; }

        /// <summary>
        /// 哨位台编号
        /// </summary>
        public int SentinelCode { get; set; }

        /// <summary>
        /// 指纹特征码
        /// </summary>
        public byte[] ByteFinger { get; set; }

        /// <summary>
        /// 指纹特征码长度
        /// </summary>
        public int FingerLength { get; set; }

        /// <summary>
        /// 人员指纹编号
        /// </summary>
        public int FingerId { get; set; }

        /// <summary>
        /// 人员指纹权限 0-查哨  1-换岗
        /// </summary>
        public int DutyType { get; set; }


    }

}
