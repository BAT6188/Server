using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace PAPS.Data
{
    /// <summary>
    /// 查勤包进度
    /// </summary>
    public class DutyCheckPackageProcess
    {

        /// <summary>
        /// 查勤时段--开始时间
        /// </summary>
        public DateTime StartTime { get; set; }

        /// <summary>
        /// 查勤时段--结束时间
        /// </summary>
        public DateTime EndTime { get; set; }


        /// <summary>
        /// 查勤包总数
        /// </summary>
        public int Total { get; set; }


        /// <summary>
        ///已完成总数
        /// </summary>
        public int CompetedCount
        { get; set; }


        /// <summary>
        /// 进度状态{0:尚未开始；1：完成当前时段查勤；2：查勤进度变更；3：切换查勤时段}
        /// </summary>
        public int ProcessStatus { get; set; }


        /// <summary>
        /// 进度描述
        /// </summary>
        public string Description { get; set; }

    }
}
