using Infrastructure.Model;
using System;

namespace AlarmAndPlan.Model
{
    /// <summary>
    /// 预定义动作
    /// </summary>
    public class PredefinedAction
    {
        public Guid PredefinedActionId
        {
            get;set;
        }

        /// <summary>
        /// 动作参数
        /// </summary>
        public string ActionArgument
        {
            get;set;
        }

        /// <summary>
        /// 动作
        /// </summary>
        public Guid ActionId
        {
            get; set;
        }

        /// <summary>
        /// 动作
        /// </summary>
        public virtual SystemOption Action { get; set; }
    }
}
